    },
    "android": {
      "package": "com.verisurfsoftware.verisurfcompanion",
      "versionCode": 60,
      "googleServicesFile": "./google-services.json",
      "blockedPermissions": [
        "android.permission.REQUEST_INSTALL_PACKAGES "
      ]
    },
    "description": "",
    "extra": {
      "eas": {
        "projectId": "ec887f1c-2cf6-44a8-96ee-d8739c25ebec"
